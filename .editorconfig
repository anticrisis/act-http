root = true

[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8
indent_style = space
indent_size = 4

[*.{cpp,h}]
indent_style = space
indent_size = 2

[*.{js,ts}]
indent_style = space
indent_size = 2

[*.{vue}]
indent_style = space
indent_size = 2

[*.{sql}]
indent_style = space
indent_size = 4

[Makefile]
indent_style = tab
indent_size = 8